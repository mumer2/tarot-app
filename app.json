{
  "expo": {
    "name": "Tarot Station",
    "slug": "tarot-station",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/TarotLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/TarotLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "21",
      "bundleIdentifier": "com.mumermalik.tarotapp",
      "infoPlist": {
    "ITSAppUsesNonExemptEncryption": false,
    "NSCameraUsageDescription": "This app uses the camera for uploading your profile image.",
    "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to choose profile images."
  }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/TarotLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.umermalik.tarotstation"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "b4aed2df-a3b1-4667-aaf6-181b7cffb41f"
      }
    }
  }
}
